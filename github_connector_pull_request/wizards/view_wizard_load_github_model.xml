<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record
    id="view_load_github_model_form_inherit"
    model="ir.ui.view">
        <field name="name">wizard.load.github.model.form.inherit</field>
        <field name="model">wizard.load.github.model</field>
        <field
        name="inherit_id"
        ref="github_connector.view_wizard_load_github_model_form" />
        <field
        name="arch"
        type="xml">
            <field
            name="github_type"
            position="after">
                <field
                name="repository_id"
                attrs="{'invisible': [('github_type', '!=', 'pr')]}" />
            </field>
        </field>
    </record>
</odoo>
